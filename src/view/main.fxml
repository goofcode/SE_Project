<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <GridPane gridLinesVisible="true" prefHeight="500.0" prefWidth="640.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <BorderPane>
               <center>
                  <ListView />
               </center>
               <top>
                  <FlowPane hgap="8.0" prefHeight="16.0" prefWidth="350.0">
                     <children>
                      <Button minWidth="70.0" mnemonicParsing="false" text="Load" />
                        <Button minWidth="70.0" mnemonicParsing="false" text="Edit" />
                        <Button minWidth="70.0" mnemonicParsing="false" text="Save" />
                     </children>
                     <BorderPane.margin>
                        <Insets bottom="7.0" left="10.0" top="7.0" />
                     </BorderPane.margin>
                  </FlowPane>
               </top>
            </BorderPane>
            <BorderPane GridPane.columnIndex="1">
               <center>
                  <ListView />
               </center>
               <top>
                  <FlowPane hgap="8.0" prefHeight="16.0" prefWidth="350.0" BorderPane.alignment="CENTER">
                     <children>
                        <Button minWidth="70.0" mnemonicParsing="false" text="Load" />
                        <Button minWidth="70.0" mnemonicParsing="false" text="Edit" />
                        <Button minWidth="70.0" mnemonicParsing="false" text="Save" />
                     </children>
                     <BorderPane.margin>
                        <Insets bottom="7.0" left="10.0" top="7.0" />
                     </BorderPane.margin>
                  </FlowPane>
               </top>
            </BorderPane>
         </children>
      </GridPane>
   </center>
   <top>
      <Button mnemonicParsing="false" text="Compare" BorderPane.alignment="TOP_LEFT">
         <BorderPane.margin>
            <Insets bottom="7.0" left="10.0" top="7.0" />
         </BorderPane.margin>
      </Button>
   </top>
</BorderPane>
